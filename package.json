{
  "name": "swan",
  "type": "module",
  "source": "src-swan/entry/*",
  "exports": {
    "require": "./public/swan-build/main.cjs",
    "default": "./public/swan-build/main.modern.js"
  },
  "main": "./public/swan-build/main.cjs",
  "module": "./public/swan-build/main.module.js",
  "unpkg": "./public/swan-build/main.umd.js",
  "files": [
    "public/swan-build"
  ],
  "scripts": {
    "start": "next start",
    "dev": "concurrently \"npm run webpackWatch\" \"npm run liveDev\" \"npm run next-dev\"",
    "next-dev": "next dev",
    "next-build": "next build",
    "build": "npm run webpackBuild; npm run next-build; ",
    "liveDev": "nodemon ./src-swan/live-dev.mjs --watch src-swan",
    "buildMicrobundle": "microbundle --jsx window.React.createElement --tsconfig tsconfig.json --globals three=window.Joy --jsxFragment window.React.Fragment --no-sourcemap --external 'none'",
    "watchMicrobundle": "microbundle watch --jsx window.React.createElement --tsconfig tsconfig.json --jsxFragment window.React.Fragment --no-sourcemap --external 'none'",
    "watchMicrobundle2": "microbundle watch --jsx window.React.createElement --tsconfig tsconfig.json --globals three=window.Joy --jsxFragment window.React.Fragment --no-sourcemap --external 'none'",
    "lint": "next lint",
    "webpackWatch": "webpack --watch --config ./webpack.dev.config.cjs --mode development",
    "webpackBuild": "webpack --config ./webpack.config.cjs --mode production"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@react-three/drei": "^9.80.2",
    "@react-three/fiber": "^8.13.6",
    "@react-three/postprocessing": "^2.15.0",
    "@react-three/xr": "^5.6.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@swc/core": "^1.3.80",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.155.1",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "daisyui": "^3.6.3",
    "es-module-shims": "^1.8.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.17.1",
    "lowdb": "^6.0.1",
    "md5": "^2.3.0",
    "microbundle": "^0.15.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.28",
    "postcss-load-config": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.1",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.7.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.3.3",
    "three": "^0.155.0",
    "three-stdlib": "^2.24.2",
    "tunnel-rat": "^0.1.2",
    "vercel": "^32.0.1",
    "vite": "^4.4.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "zustand": "^4.4.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.22.5",
    "@rollup/plugin-replace": "^5.0.2",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  }
}