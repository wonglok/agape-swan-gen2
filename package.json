{
  "name": "swan",
  "type": "module",
  "source": "src-swan/entry/*",
  "exports": {
    "require": "./public/swan-build/main.cjs",
    "default": "./public/swan-build/main.modern.js"
  },
  "main": "./public/swan-build/main.cjs",
  "module": "./public/swan-build/main.module.js",
  "unpkg": "./public/swan-build/main.umd.js",
  "files": [
    "public/swan-build"
  ],
  "scripts": {
    "start": "next start",
    "dev": "concurrently \"npm run watchCode\" \"npm run liveDev\" \"npm run next-dev\"",
    "next-dev": "next dev",
    "next-build": "next build",
    "build": "npm run buildCode; npm run next-build; ",
    "liveDev": "nodemon ./src-swan/live-dev.mjs --watch src-swan",
    "buildCode": "microbundle --jsx window.React.createElement --tsconfig tsconfig.json --globals three=window.Joy --jsxFragment window.React.Fragment --no-sourcemap --external 'none'",
    "watchCode": "microbundle watch --jsx window.React.createElement --tsconfig tsconfig.json --globals three=window.Joy --jsxFragment window.React.Fragment --no-sourcemap --external 'none'",
    "lint": "next lint"
  },
  "devDependencies": {
    "@react-three/drei": "^9.80.2",
    "@react-three/fiber": "^8.13.6",
    "@react-three/postprocessing": "^2.15.0",
    "@react-three/xr": "^5.6.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "es-module-shims": "^1.8.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.17.1",
    "lowdb": "^6.0.1",
    "md5": "^2.3.0",
    "microbundle": "^0.15.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.28",
    "postcss-load-config": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.3.3",
    "three": "^0.155.0",
    "three-stdlib": "^2.24.2",
    "tunnel-rat": "^0.1.2",
    "vercel": "^32.0.1",
    "vite": "^4.4.5",
    "zustand": "^4.4.1"
  },
  "dependencies": {
    "daisyui": "^3.6.3",
    "next": "^13.4.19",
    "next-auth": "^4.23.1"
  }
}